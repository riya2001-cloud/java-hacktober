class Main{
  public static void main(String[] args){
    int a[] ={1,3,-4,2,4,-6,7,1,2,0};
    int cursum=0,maxsum=0;
    for(int i=0;i<a.length;i++){
      cursum=cursum+a[i];
      if(cursum<0){
        cursum=0;
      }
      maxsum=Math.max(cursum,maxsum);
    }
    System.out.println( maxsum);
  }
}
